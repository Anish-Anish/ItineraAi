# .env.example - Copy this to .env and fill in your actual values

# Google Maps API Configuration
# This key is used for displaying maps in the Journey Flow feature
REACT_APP_GOOGLE_MAPS_API_KEY=your-google-maps-api-key

# If using Vite instead of Create React App, use this:
VITE_REACT_APP_GOOGLE_MAPS_API_KEY=your-google-maps-api-key

# Backend URL
REACT_APP_BACKEND_URL=http://localhost:8089
